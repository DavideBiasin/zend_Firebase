{
  "name" : "zend_Firebase/zend_Firebase",
  "description" : "firebase library",
  "license" : "BSD-3-Clause",
  "keywords" : [ "firebase", "firebase library", "google firebase library", "php firebase", "php library firebase", "REST", "wrapper","php7 firebase", "REST firebase", "REST php7 firebase"],
  "homepage" : "http://framework.zend.com/",
  "prefer-stable" : true,
  "require" : {
    "php" : ">=7.0",
    "guzzlehttp/guzzle" : "6.2.2",
    "guzzlehttp/psr7" : "1.3.1",
    "friendsofphp/php-cs-fixer" : "v1.12.2"
  },
  "require-dev" : {
    "phpunit/PHPUnit" : "5.3.1",
    "squizlabs/php_codesniffer" : "2.7.0"
  },
  "repositories" : [ {
    "type" : "composer",
    "url" : "https://packagist.org/"
  } ],
  "autoload-dev" : {
    "psr-4" : {
      "ZendFirebaseTest\\Firebase\\" : "tests/"
    }
  },
  "minimum-stability" : "dev",
  "autoload" : {
    "psr-4" : {
      "ZendFirebase\\Firebase\\" : "src/"
    }
  },
  "scripts" : {
    "cs-check" : "phpcs",
    "cs-fix" : "phpcbf",
    "test" : "phpunit",
    "test-coverage" : "phpunit --coverage-clover clover.xml"
  },
